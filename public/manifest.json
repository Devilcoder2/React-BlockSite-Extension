{
  "manifest_version": 3,
  "name": "React Block Sites",
  "version": "0.0.1",
  "icons": {
    "48": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "./index.html"
  },
  "permissions": ["activeTab", "storage"]
}
